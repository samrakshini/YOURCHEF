<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.yourchef">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_launch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_screen" />
        <activity android:name=".Register_here" />
        <activity
            android:name=".Chef_pane"
            android:label="@string/title_activity_chef_pane"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Client_pane"
            android:label="@string/title_activity_client_pane"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>